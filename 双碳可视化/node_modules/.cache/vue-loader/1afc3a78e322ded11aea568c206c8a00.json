localhost:9090/carbonReport/productPowerReport/month\").then(res => {\r\n        console.log(res);\r\n        this.config5 = {\r\n          number: [res.data.rows[0].totalCarbonEmission],\r\n          toFixed: 2,\r\n          content: '{nt}'\r\n        };\r\n      });\r\n      this.$axios.get(\"http://localhost:9090/carbonReport/productPowerReport/day\").then(res => {\r\n        console.log(res);\r\n        this.config6 = {\r\n          number: [res.data.rows[0].totalCarbonEmission],\r\n          toFixed: 2,\r\n          content: '{nt}'\r\n        };\r\n      });\r\n      this.$axios.get(\"http://localhost:9090/carbonReport/equipmentPowerReport/list\").then(res => {\r\n        console.log(res);\r\n        let abc = res.data.rows;\r\n        this.config7 = {\r\n          header: [\"编号\", \"设备\", \"生产数量\", \"碳排放\"],\r\n          data: []\r\n        };\r\n        for (let i = 0; i < 8; i++) {\r\n          this.config7.data.push([i + 1, abc[i].equipmentName, abc[i].productQuantity, abc[i].totalCarbonEmission]);\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.top {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n.yzh {\r\n  height: 50px;\r\n}\r\n\r\n#Bottom {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center; /* 垂直居中 */\r\n  justify-content: center; /* 水平居中 */\r\n  margin-top: 20px;\r\n}\r\n\r\n.dv-scroll-board {\r\n  height: 280px;\r\n  width: 680px;\r\n  margin-left: 20px;\r\n  margin-right: 20px;\r\n}\r\n</style>\r\n"]}]}