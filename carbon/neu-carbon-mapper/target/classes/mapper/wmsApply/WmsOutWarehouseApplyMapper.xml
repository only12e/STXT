    <insert id="batchWmsOutWarehouseApplyDetail">
        insert into wms_out_warehouse_apply_detail(  apply_id, material_id, warehouse_id, wh_location_id, wh_region_id, out_quantity, serial_no, batch_no, product_date, manufacturer) values
        <foreach item="item" index="index" collection="list" separator=",">
            (  #{item.applyId}, #{item.materialId}, #{item.warehouseId}, #{item.whLocationId}, #{item.whRegionId}, #{item.outQuantity}, #{item.serialNo}, #{item.batchNo}, #{item.productDate}, #{item.manufacturer})
