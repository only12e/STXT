    <resultMap id="WmsOutWarehouseApplyWmsOutWarehouseApplyDetailResult" type="WmsOutWarehouseApply" extends="WmsOutWarehouseApplyResult">
        <collection property="wmsOutWarehouseApplyDetailList" notNullColumn="id" javaType="java.util.List" resultMap="WmsOutWarehouseApplyDetailResult" />
