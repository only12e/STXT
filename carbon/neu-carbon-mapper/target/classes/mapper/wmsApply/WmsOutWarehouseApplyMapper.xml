    <sql id="selectWmsOutWarehouseApplyVo">
        select id, serial_no, biz_bill_id, biz_type, customer_id, contract_id, contract_type, apply_type, apply_user, apply_time, apply_status, audit_user, audit_time, audit_status, audit_comment, remark, bill_status from wms_out_warehouse_apply
    </sql>

    <select id="selectWmsOutWarehouseApplyList" parameterType="WmsOutWarehouseApply" resultMap="WmsOutWarehouseApplyResult">
        <include refid="selectWmsOutWarehouseApplyVo"/>
        <where>  
            <if test="serialNo != null  and serialNo != ''"> and serial_no like concat('%', #{serialNo}, '%')</if>
            <if test="bizBillId != null "> and biz_bill_id = #{bizBillId}</if>
            <if test="bizType != null  and bizType != ''"> and biz_type = #{bizType}</if>
            <if test="customerId != null "> and customer_id = #{customerId}</if>
            <if test="contractId != null "> and contract_id = #{contractId}</if>
            <if test="contractType != null  and contractType != ''"> and contract_type = #{contractType}</if>
            <if test="applyType != null  and applyType != ''"> and apply_type = #{applyType}</if>
            <if test="applyUser != null  and applyUser != ''"> and apply_user = #{applyUser}</if>
            <if test="params.beginApplyTime != null and params.beginApplyTime != '' and params.endApplyTime != null and params.endApplyTime != ''"> and apply_time between #{params.beginApplyTime} and #{params.endApplyTime}</if>
            <if test="applyStatus != null  and applyStatus != ''"> and apply_status = #{applyStatus}</if>
            <if test="auditUser != null  and auditUser != ''"> and audit_user = #{auditUser}</if>
            <if test="params.beginAuditTime != null and params.beginAuditTime != '' and params.endAuditTime != null and params.endAuditTime != ''"> and audit_time between #{params.beginAuditTime} and #{params.endAuditTime}</if>
            <if test="auditStatus != null  and auditStatus != ''"> and audit_status = #{auditStatus}</if>
            <if test="billStatus != null  and billStatus != ''"> and bill_status = #{billStatus}</if>
            <if test="params.applyStatusList != null"> and apply_status in
            	<foreach collection="params.applyStatusList" item="applyStatus" open="(" close=")" separator=",">
            		#{applyStatus}
            	</foreach>
            </if>
        </where>
    </select>
    
    <select id="selectWmsOutWarehouseApplyById" parameterType="Long" resultMap="WmsOutWarehouseApplyWmsOutWarehouseApplyDetailResult">
        select a.id, a.serial_no, a.biz_bill_id, a.biz_type, a.customer_id, a.contract_id, a.contract_type, a.apply_type, a.apply_user, a.apply_time, a.apply_status, a.audit_user, a.audit_time, a.audit_status, a.audit_comment, a.remark, a.bill_status,
            b.id, b.apply_id, b.material_id, b.warehouse_id, b.wh_location_id, b.wh_region_id, b.out_quantity, b.serial_no, b.batch_no, b.product_date, b.manufacturer
        from wms_out_warehouse_apply a
        left join wms_out_warehouse_apply_detail b on b.apply_id = a.id
        where a.id = #{id}
    </select>
        
    <insert id="insertWmsOutWarehouseApply" parameterType="WmsOutWarehouseApply" useGeneratedKeys="true" keyProperty="id">
        insert into wms_out_warehouse_apply
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="serialNo != null">serial_no,</if>
            <if test="bizBillId != null">biz_bill_id,</if>
            <if test="bizType != null">biz_type,</if>
            <if test="customerId != null">customer_id,</if>
            <if test="contractId != null">contract_id,</if>
            <if test="contractType != null">contract_type,</if>
            <if test="applyType != null">apply_type,</if>
            <if test="applyUser != null">apply_user,</if>
            <if test="applyTime != null">apply_time,</if>
            <if test="applyStatus != null">apply_status,</if>
            <if test="auditUser != null">audit_user,</if>
            <if test="auditTime != null">audit_time,</if>
            <if test="auditStatus != null">audit_status,</if>
            <if test="auditComment != null">audit_comment,</if>
            <if test="remark != null">remark,</if>
            <if test="billStatus != null">bill_status,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
